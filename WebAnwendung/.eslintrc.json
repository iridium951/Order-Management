{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", {
            "code": 120
        }],
        "id-length": ["error", {
            "min": 3
        }],
        "linebreak-style": ["error", "windows"]
    }
}
